Sub JTR()

Dim YTD As Worksheet
Dim Week As Worksheet
Dim i As Integer
Dim r As Integer

Set YTD = ActiveWorkbook.Sheets("Item YTD")
Set Week = ActiveWorkbook.Sheets("Item Week")

YTD.Columns("A:F").Delete Shift:=xlToRight
    
'With Sheets("Item YTD")
